<div class="container p-5">
    {{#if tx}}
    {{#each tx}}
    <div class=" col align-self-center p-2">
        <div class="card-header bg-dark text-white text-center">
            <h5 class="card-title">{{ id }} <small class="text-muted"></small></h5>
        </div>
        <ul class="list-group list-group-flush">
            <li class="list-group-item">
                <span class="">Llave Publica</span>
                <div class="text-truncate" style="color:rgba(16, 124, 138, 0.767);">
                    <small>{{ input.address }}</small>
                </div>
                <span class="">Cantidad</span>
                <div class="text-truncate" style="color:rgba(16, 124, 138, 0.767);">
                    <small>{{ input.amount }}</small>
                </div>
                <span class="">Tiempo</span>
                <div class="text-truncate" style="color:rgba(16, 124, 138, 0.767);">
                    <small>{{ input.timestamp }}</small>
                </div>
            </li>
            <li class="list-group-item">
                {{#each outputs}}
                <span class="">Llave Publica</span>
                <div class="text-truncate" style="color:rgba(31, 77, 17, 0.664);">
                    <small>{{ address }}</small>
                </div>
                <span class="">Cantidad</span>
                <div class="text-truncate" style="color:rgba(31, 77, 17, 0.664);">
                    <small>{{ amount }}</small>
                </div>
                {{/each}}
            </li>
        </ul>
    </div>
    {{/each}}
    {{else}}
    <div class="card-header bg-dark text-white text-center">
        <h5 class="card-title">No existe la transaci√≥n<small class="text-muted"></small></h5>
    </div>
    {{/if}}
</div>